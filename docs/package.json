{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf resources/docbook-xsl ./EN_US/common ./build",
    "expand-docbook": "tar xjf ./resources/docbook-xsl.tar.bz2 -C ./resources",
    "copy-version": "cpx ./common/Version.xml ./EN_US/common",
    "copy-images": "cpx './EN_US/images/**/*' ./build/images",
    "copy-css": "cpx ./common/LN-html.css ./build/DataTutorial/",
    "copy": "run-p copy-version copy-images copy-css",
    "gen-DataTutorial": "xsltproc --output ./build/DataTutorial/ --xinclude ./BuildTools/PortalGen2.xsl ./EN_US/HPCCDataTutorial/DataTutorial.xml",
    "build": "run-s expand-docbook copy gen-DataTutorial",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}