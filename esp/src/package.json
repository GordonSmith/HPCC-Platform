{
  "name": "eclwatch",
  "private": true,
  "version": "1.0.0",
  "description": "'ECL Watch' Web interface for HPCC Platform.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf ./build ./lib ./lib-es5 ./types ./types-es5 ./src/nlsHPCCType.ts",
    "lint": "eslint ./eclwatch ./src ./src-react",
    "lint-fix": "eslint --fix eclwatch/**/*.js src/**/*.ts src-react/**/*.ts?",
    "copy-res-eclwatch-img": "copyfiles -u 2 \"./eclwatch/img/**/*.{png,jpg,gif,svg}\" ./build/eclwatch/img/",
    "copy-res-eclwatch-ecl": "copyfiles -u 2 \"./eclwatch/ecl/**/*.*\" ./build/eclwatch/ecl/",
    "copy-res-dgrid-shim": "copyfiles -u 4 \"./node_modules/@hpcc-js/dgrid-shim/dist/index.min.*\" ./build/node_modules/@hpcc-js/dgrid-shim/dist/",
    "copy-res-dojo-img": "copyfiles -u 3 \"./node_modules/dojo/resources/blank.gif\" ./build/eclwatch/img/",
    "copy-res-dojo": "copyfiles -u 3 \"./node_modules/dojo/resources/**/*.{png,jpg,gif}\" ./build/node_modules/dojo/resources/",
    "copy-res-dojox": "copyfiles -u 5 \"./node_modules/dojox/widget/ColorPicker/images/**/*.{png,jpg,gif}\" ./build/eclwatch/img/",
    "copy-res-TopoJSON": "copyfiles -u 4 \"./node_modules/@hpcc-js/map/TopoJSON/**/*\" ./build/dist/TopoJSON/",
    "copy-res-font-awesome": "copyfiles -u 2 \"./node_modules/font-awesome/**/*\" ./build/dist/font-awesome/",
    "copy-res-fluentui-fonts": "copyfiles -f \"./node_modules/@fluentui/font-icons-mdl2/fonts/*\" ./build/dist/fluentui-fonts/",
    "copy-res-html": "copyfiles \"./*.{htm,html}\" ./build/",
    "copy-res": "run-p copy-res-eclwatch-img copy-res-eclwatch-ecl copy-res-dgrid-shim copy-res-dojo-img copy-res-dojo copy-res-dojox copy-res-TopoJSON copy-res-font-awesome copy-res-fluentui-fonts copy-res-html",
    "compile": "tsc -p ./tsconfig.json",
    "compile-modern": "tsc",
    "compile-watch": "npm run compile -- -w",
    "bundle": "node node_modules/webpack/bin/webpack.js --env production --config webpack.config.js",
    "bundle-dev": "node node_modules/webpack/bin/webpack.js --env development --config webpack.config.js",
    "bundle-watch": "npm run bundle-dev -- --watch",
    "build": "npm-run-all --parallel copy-res compile --serial bundle",
    "build-dev": "npm-run-all --parallel copy-res compile --serial bundle-dev",
    "dev-start-ws": "ws",
    "dev-start": "run-p bundle-watch dev-start-ws",
    "dev-start-verbose": "ws --verbose.include request response",
    "rm-hpcc": "rimraf ./node_modules/@hpcc-js",
    "start": "webpack serve --env development --config webpack.config.js",
    "test": "run-s lint",
    "update": "npx npm-check-updates -u -t minor",
    "update-major": "npx npm-check-updates -u"
  },
  "main": "src/stub.js",
  "dependencies": {
    "@fluentui/react": "8.119.3",
    "@fluentui/react-components": "9.54.6",
    "@fluentui/react-experiments": "8.14.160",
    "@fluentui/react-hooks": "8.8.10",
    "@fluentui/react-icons-mdl2": "1.3.72",
    "@fluentui/react-migration-v8-v9": "9.6.22",
    "@hpcc-js/chart": "2.86.0",
    "@hpcc-js/codemirror": "2.64.0",
    "@hpcc-js/common": "2.73.0",
    "@hpcc-js/comms": "2.99.0",
    "@hpcc-js/dgrid": "2.34.0",
    "@hpcc-js/dgrid-shim": "2.26.0",
    "@hpcc-js/dataflow": "9.3.0",
    "@hpcc-js/eclwatch": "2.76.1",
    "@hpcc-js/graph": "2.87.0",
    "@hpcc-js/html": "2.44.0",
    "@hpcc-js/layout": "2.51.0",
    "@hpcc-js/map": "2.79.0",
    "@hpcc-js/other": "2.17.0",
    "@hpcc-js/phosphor": "2.20.0",
    "@hpcc-js/react": "2.55.0",
    "@hpcc-js/timeline": "2.54.0",
    "@hpcc-js/tree": "2.42.0",
    "@hpcc-js/util": "2.53.0",
    "@hpcc-js/wasm-duckdb": "1.5.1",
    "@kubernetes/client-node": "0.20.0",
    "clipboard": "2.0.11",
    "d3-dsv": "3.0.1",
    "dijit": "1.17.3",
    "dijit-themes": "1.17.3",
    "dojo": "1.17.3",
    "dojo-util": "1.17.3",
    "dojox": "1.17.3",
    "es6-promise": "4.2.8",
    "font-awesome": "4.7.0",
    "formik": "2.4.5",
    "octokit": "3.1.2",
    "put-selector": "0.3.6",
    "query-string": "7.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.51.2",
    "react-hot-toast": "2.4.1",
    "react-reflex": "4.2.6",
    "react-singleton-hook": "3.4.0",
    "react-sizeme": "3.0.2",
    "universal-router": "9.2.0",
    "xstyle": "0.3.3"
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@types/dojo": "1.9.48",
    "@types/react": "17.0.80",
    "@types/react-dom": "17.0.25",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react-swc": "3.7.2",
    "copyfiles": "2.4.1",
    "css-loader": "7.1.2",
    "dojo-webpack-plugin": "3.0.6",
    "eslint": "8.57.0",
    "eslint-plugin-eclwatch": "file:./eslint",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "local-web-server": "5.3.1",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "source-map-loader": "5.0.0",
    "string-replace-loader": "3.1.0",
    "style-loader": "4.0.0",
    "tslib": "2.6.2",
    "typescript": "5.4.3",
    "url-loader": "4.1.1",
